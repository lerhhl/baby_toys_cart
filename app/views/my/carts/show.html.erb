<div class="container">
  <div class="row">
    <div class="col-md-6">
      <h1> Cart </h1>
        <ol>
          <% @cartlist.each do |list| %>
          <% @product = Product.find(list.product_id) %>
          <li><%= @product.name %>  <%= list.purchase_quantity %></li>
          <% end %>
        </ol>
      <%= link_to "Back", :back %>
    </div>
  </div>
</div>
